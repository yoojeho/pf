{   
    "extends" : "airbnb",
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": [
            "error",
            "windows"
        ],
        "indent": [
            "error", "tab"
        ],
        "no-tabs": 0,
        "no-plusplus": 0,
        "camelcase": 0,
        "no-console": 0,
        "no-shadow": [
            "error",
            {
                "builtinGlobals": false,
                "hoist": "functions",
                "allow": ["err", "error", "sql", "results", "result"]
            }
        ],
        
        "object-shorthand": 0,
        "prefer-template":0
    },
    "globals": {
        "jQuery": true,
        "$": true
    }
}